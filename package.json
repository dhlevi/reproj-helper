{
  "name": "reproj-helper",
  "version": "1.0.3",
  "description": "Helper library for reprojecting geojson geometries with proj4js",
  "author": {
    "name": "dhlevi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhlevi/reproj-helper.git"
  },
  "scripts": {
    "build-ts": "tsc",
